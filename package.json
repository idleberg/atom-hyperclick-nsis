{
  "name": "hyperclick-nsis",
  "version": "0.9.0",
  "description": "Hyperclick provider for Nullsoft Scriptable Install System (NSIS)",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "npm run start",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run build",
    "start": "webpack --mode none --watch",
    "test": "npm run lint"
  },
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "nsis",
    "hyperclick"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-hyperclick-nsis.git"
  },
  "main": "./lib/hyperclick-nsis",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "activationHooks": [
    "language-nsis:grammar-used"
  ],
  "providedServices": {
    "hyperclick": {
      "versions": {
        "0.1.0": "getProvider"
      }
    }
  },
  "package-deps": [
    "atom-ide-hyperclick"
  ],
  "dependencies": {
    "atom-package-deps": "^6.0.0",
    "atom-satisfy-dependencies": "^0.2.1",
    "makensis": "^0.21.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.4.0",
    "husky": "^4.2.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
