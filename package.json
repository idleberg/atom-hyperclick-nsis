{
  "name": "hyperclick-nsis",
  "version": "0.5.0",
  "description": "Hyperclick provider for Nullsoft Scriptable Install System (NSIS)",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode none --watch",
    "lint": "eslint ./src",
    "test": "npm run lint"
  },
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "nsis",
    "hyperclick"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-hyperclick-nsis.git"
  },
  "main": "./lib/hyperclick-nsis",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "activationHooks": [
    "language-nsis:grammar-used"
  ],
  "providedServices": {
    "hyperclick.provider": {
      "versions": {
        "0.0.0": "getProvider"
      }
    }
  },
  "package-deps": [
    "hyperclick"
  ],
  "dependencies": {
    "atom-package-deps": "^5.1.0",
    "makensis": "^0.18.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "eslint": "^6.1.0",
    "husky": "^3.0.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
